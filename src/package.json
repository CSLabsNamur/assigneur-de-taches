      "icon": "build/icon.ico"
    },
    "directories": {
      "output": "build"
    }
  },
  "devDependencies": {
    "bootstrap": "^4.4.1",
    "electron": "^8.2.2",
    "electron-builder": "^22.5.1",
    "electron-reload": "^1.5.0"
  }
}